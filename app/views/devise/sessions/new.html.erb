<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <span class="input-group-addon" id="basic-addon1"><%= f.label :email %></span>
    <%= f.email_field :email, autofocus: true, :class => "form-control", :value => "dorian_collier@medhurst.us"%>
  </div>

  <div class="field">
    <span class="input-group-addon" id="basic-addon1"><%= f.label :password %></span>
    <%= f.password_field :password, autocomplete: "off", :class => "form-control", :value => "happyhappy"%>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <p><%= f.check_box :remember_me%>
      <%= f.label :remember_me%></p>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in",  :class => "btn btn-default", :id => "login_submit"%>
  </div>
<% end %>

<%= render "devise/shared/links" %>
